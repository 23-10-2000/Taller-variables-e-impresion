Algoritmo Taller_de_variables_e_impresion_4
	
	Definir nombreCiudadCapital Como Caracter;
	Definir nombrePais Como Caracter;
	
	Escribir "Escriba el nombre de una ciudad capital: ";
	Leer nombreCiudadCapital;
	
	Escribir "Escriba el nombre del pais: ";
	Leer nombrePais;
	
	Limpiar Pantalla;
	
	Escribir "La ciudad ", nombreCiudadCapital, " es la capital del pais ", nombrePais;

	
FinAlgoritmo
